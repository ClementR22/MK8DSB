{
  "compilerOptions": {
    "target": "ES6",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "extends": "expo/tsconfig.base",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app",
    "components",
    "babel.config.js",
    "stores/usePressableElementsStore.ts",
    "translations"
  ]
}
